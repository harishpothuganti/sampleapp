<process name="BPELProcess1"
               targetNamespace="http://xmlns.oracle.com/salesforce_application/Project1/BPELProcess1"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/salesforce_application/Project1/BPELProcess1"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://soap.sforce.com/2005/09/outbound" xmlns:ui="http://xmlns.oracle.com/soa/designer">

  <!--
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC
     Set of activities coordinating the flow of messages across the
     services integrated within this business process
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://soap.sforce.com/2005/09/outbound" location="../WSDLs/harish_outboundWrapper.wsdl"
          importType="http://schemas.xmlsoap.org/wsdl/" ui:processWSDL="true"/>
  <partnerLinks>
    <partnerLink name="SOAPService" partnerLinkType="ns1:SOAPService" myRole="NotificationPort"/>
  </partnerLinks>
  <variables>
    <variable name="Receive1_notifications_InputVariable" messageType="ns1:notificationsRequest"/>
  </variables>
  <sequence name="main">
    <receive name="Receive1" createInstance="yes" partnerLink="SOAPService" portType="ns1:NotificationPort"
             operation="notifications" variable="Receive1_notifications_InputVariable"/>
  </sequence>
</process>